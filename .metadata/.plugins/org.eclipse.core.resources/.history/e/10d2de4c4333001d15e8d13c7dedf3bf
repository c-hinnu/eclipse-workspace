package Searching;

public class Linear {

	public static void main(String[] args) {
		int [] a= {36,19,29,12,5};
		int i;
		int item=12;
		for(i=0;i<a.length;i++)
		{
			if(a[i]==item)
			{
				System.out.println("item found at position"+i);
				break;
			}
			else
			{
				System.out.println("not found");
			}
		}

	}

}
